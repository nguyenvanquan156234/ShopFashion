@model IEnumerable<BanHangOnline.Models.EF.Category>
@if (Model != null && Model.Any())
{
    <ul class="navbar_menu">
        @foreach (var item in Model)
        {
            if (item != null && item.Alias != null && item.Alias.Equals("trang-chu"))
            {
                item.Alias = "";

            }
            <li><a href="/@item.Alias">@item.Title</a></li>
        }
     
    </ul>
}
